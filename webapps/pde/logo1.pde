	    	 bolas[i].animar();
	      }else{
	    	  PVector ref = new PVector(p[i][0],p[i][1]);
	    	 bolas[i].seguir(p[i][0],p[i][1]);
	    	 if(PVector.dist(bolas[i].pos, ref)>3){
	    		 bolas[i].animar();
	    	 }else{
	    		 bolas[i].pos.set(ref);
	    	 }
	      }
	  }
	  
	  if(frameCount%timeout==0){
		  animar=!animar;
		  timeout=(int)(random(1,5)*100);
		  if(animar){
			  for(int i=0;i<bolas.length;i++){
				  bolas[i].vel = new PVector(random(-1f, 1f), random(0, 5));
				  bolas[i].acc = PVector.fromAngle(random(TWO_PI));
			  }
		  }
	  }
	  
	  //rango = 50*mouseX/width;
	}


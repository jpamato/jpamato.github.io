	public class Bola_ {

		PVector pos, vel, acc;
		int tam;

		Bola_(PVector p) {
			pos = new PVector(p.x,p.y);
			vel = new PVector(random(-0.5f, 0.5f), random(0, 5));
			acc = PVector.fromAngle(random(TWO_PI));
			tam = 10;
		}
		
		Bola_() {
			pos = new PVector(random(width), random(height));
			vel = new PVector(random(-5, 5), random(-5, 5));
			acc = PVector.fromAngle(random(TWO_PI));
			tam = 10;
		}

		Bola_(int t) {
			pos = new PVector(random(width),
					random(height));
			vel = new PVector(random(-5, 5), random(-5, 5));
			acc = PVector.fromAngle(random(TWO_PI));
			tam = t;
		}

		public void animar() {
			if (vel.mag() > 5) {
				vel.normalize();
				vel.mult(5);

			}
			pos.add(vel);
			if (pos.x > width) {
				pos.x = 0;
			} else if (pos.x < 0) {
				pos.x = width;
			}
			if (pos.y > height) {
				pos.y = 0;
			} else if (pos.y < 0) {
				pos.y = height;
			}
		}

		public void sacudir() {
			vel.add(acc);
			acc = PVector.fromAngle(random(TWO_PI));
		}

		public void seguir(PVector target) {
			acc = PVector.sub(target, pos);
			vel.add(acc);
		}

		public void seguir(int x, int y) {
			//PVector target = new PVector(x, y);
			//acc = PVector.sub(target, pos);
			acc.set(x-pos.x, y-pos.y);
			vel.add(acc);
		}
		
		public void seguir(float x, float y) {
			PVector target = new PVector(x, y);
			acc = PVector.sub(target, pos);
			vel.add(acc);
		}

		public void seguir(Bola_ target) {
			acc = PVector.sub(target.pos, pos);
			vel.add(acc);
		}

		public void dibujar() {
			ellipse(pos.x, pos.y, tam, tam);
		}
	}
